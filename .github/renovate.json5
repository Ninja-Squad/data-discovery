{
  extends: [
    'config:recommended',
    'group:allNonMajor',
    'schedule:weekly',
  ],
  lockFileMaintenance: {
    enabled: true,
  },
  baseBranches: [
    'chore/next',
  ],
  prConcurrentLimit: 1,
  automerge: true,
  packageRules: [
    {
      matchPackageNames: [
        'docker.elastic.co/elasticsearch/elasticsearch',
      ],
      matchUpdateTypes: [
        'major',
      ],
      enabled: false,
    },
    {
      matchPackageNames: [
        'typescript',
      ],
      matchUpdateTypes: [
        'minor',
        'major',
      ],
      enabled: false,
    },
  ],
  ignoreDeps: [
    'cimg/openjdk',
    'circleci/openjdk',
  ],
}
