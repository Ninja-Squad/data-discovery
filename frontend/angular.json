{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "pnpm"
  },
  "newProjectRoot": "projects",
  "projects": {
    "data-discovery-frontend": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "dd",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js",
              "@angular/localize/init"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "src/assets/rare",
                "followSymlinks": true,
                "ignore": [
                  "**/*.scss"
                ],
                "output": "/assets"
              }
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/assets/rare/"
              ],
              "sass": {
                "silenceDeprecations": [
                  "import"
                ]
              }
            },
            "styles": [
              "src/styles.scss",
              "node_modules/prismjs/themes/prism-okaidia.css",
              "node_modules/prismjs/plugins/line-numbers/prism-line-numbers.css"
            ],
            "scripts": [
              "node_modules/prismjs/prism.js",
              "node_modules/prismjs/components/prism-json.js",
              "node_modules/prismjs/plugins/line-numbers/prism-line-numbers.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "900kB",
                  "maximumError": "1.2MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kB"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "baseHref": "./"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            },
            "rare": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.rare.ts"
                },
                {
                  "replace": "src/environments/document.default.ts",
                  "with": "src/environments/document.rare.ts"
                },
                {
                  "replace": "src/environments/basket.default.ts",
                  "with": "src/environments/basket.actual.ts"
                },
                {
                  "replace": "src/environments/select-all-results.default.ts",
                  "with": "src/environments/select-all-results.actual.ts"
                }
              ],
              "baseHref": "/rare-dev/"
            },
            "rare-production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.rare.prod.ts"
                },
                {
                  "replace": "src/environments/document.default.ts",
                  "with": "src/environments/document.rare.ts"
                },
                {
                  "replace": "src/environments/basket.default.ts",
                  "with": "src/environments/basket.actual.ts"
                },
                {
                  "replace": "src/environments/select-all-results.default.ts",
                  "with": "src/environments/select-all-results.actual.ts"
                }
              ]
            },
            "brc4env": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.brc4env.ts"
                },
                {
                  "replace": "src/environments/document.default.ts",
                  "with": "src/environments/document.rare.ts"
                },
                {
                  "replace": "src/environments/basket.default.ts",
                  "with": "src/environments/basket.actual.ts"
                },
                {
                  "replace": "src/environments/select-all-results.default.ts",
                  "with": "src/environments/select-all-results.actual.ts"
                }
              ],
              "baseHref": "/brc4env-dev/",
              "stylePreprocessorOptions": {
                "includePaths": [
                  "src/assets/brc4env/"
                ],
                "sass": {
                  "silenceDeprecations": [
                    "import"
                  ]
                }
              },
              "assets": [
                {
                  "glob": "*",
                  "input": "src/assets/brc4env",
                  "followSymlinks": true,
                  "ignore": [
                    "**/*.scss"
                  ],
                  "output": "/assets"
                }
              ]
            },
            "brc4env-production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.brc4env.prod.ts"
                },
                {
                  "replace": "src/environments/document.default.ts",
                  "with": "src/environments/document.rare.ts"
                },
                {
                  "replace": "src/environments/basket.default.ts",
                  "with": "src/environments/basket.actual.ts"
                },
                {
                  "replace": "src/environments/select-all-results.default.ts",
                  "with": "src/environments/select-all-results.actual.ts"
                }
              ],
              "stylePreprocessorOptions": {
                "includePaths": [
                  "src/assets/brc4env/"
                ],
                "sass": {
                  "silenceDeprecations": [
                    "import"
                  ]
                }
              },
              "assets": [
                {
                  "glob": "*",
                  "input": "src/assets/brc4env",
                  "followSymlinks": true,
                  "ignore": [
                    "**/*.scss"
                  ],
                  "output": "/assets"
                }
              ]
            },
            "wheatis": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.wheatis.ts"
                },
                {
                  "replace": "src/environments/header.default.ts",
                  "with": "src/environments/header.wheatis.ts"
                }
              ],
              "baseHref": "/wheatis-dev/",
              "stylePreprocessorOptions": {
                "includePaths": [
                  "src/assets/wheatis/"
                ],
                "sass": {
                  "silenceDeprecations": [
                    "import"
                  ]
                }
              },
              "assets": [
                {
                  "glob": "*",
                  "input": "src/assets/wheatis",
                  "followSymlinks": true,
                  "ignore": [
                    "**/*.scss"
                  ],
                  "output": "/assets"
                }
              ]
            },
            "wheatis-production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.wheatis.prod.ts"
                },
                {
                  "replace": "src/environments/header.default.ts",
                  "with": "src/environments/header.wheatis.ts"
                }
              ],
              "stylePreprocessorOptions": {
                "includePaths": [
                  "src/assets/wheatis/"
                ],
                "sass": {
                  "silenceDeprecations": [
                    "import"
                  ]
                }
              },
              "assets": [
                {
                  "glob": "*",
                  "input": "src/assets/wheatis",
                  "followSymlinks": true,
                  "ignore": [
                    "**/*.scss"
                  ],
                  "output": "/assets"
                }
              ]
            },
            "faidare": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.faidare.ts"
                },
                {
                  "replace": "src/environments/document.default.ts",
                  "with": "src/environments/document.faidare.ts"
                },
                {
                  "replace": "src/environments/document-list.default.ts",
                  "with": "src/environments/document-list.faidare.ts"
                },
                {
                  "replace": "src/environments/ontology-aggregation.default.ts",
                  "with": "src/environments/ontology-aggregation.faidare.ts"
                },
                {
                  "replace": "src/environments/header.default.ts",
                  "with": "src/environments/header.faidare.ts"
                },
                {
                  "replace": "src/environments/footer.default.ts",
                  "with": "src/environments/footer.faidare.ts"
                },
                {
                  "replace": "src/environments/basket.default.ts",
                  "with": "src/environments/basket.actual.ts"
                },
                {
                  "replace": "src/environments/select-all-results.default.ts",
                  "with": "src/environments/select-all-results.actual.ts"
                },
                {
                  "replace": "src/environments/map-container.default.ts",
                  "with": "src/environments/map-container.faidare.ts"
                }
              ],
              "baseHref": "/faidare-dev/",
              "stylePreprocessorOptions": {
                "includePaths": [
                  "src/assets/faidare/"
                ],
                "sass": {
                  "silenceDeprecations": [
                    "import"
                  ]
                }
              },
              "assets": [
                {
                  "glob": "*",
                  "input": "src/assets/faidare",
                  "followSymlinks": true,
                  "ignore": [
                    "**/*.scss"
                  ],
                  "output": "/assets"
                }
              ]
            },
            "faidare-production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.faidare.prod.ts"
                },
                {
                  "replace": "src/environments/document.default.ts",
                  "with": "src/environments/document.faidare.ts"
                },
                {
                  "replace": "src/environments/document-list.default.ts",
                  "with": "src/environments/document-list.faidare.ts"
                },
                {
                  "replace": "src/environments/ontology-aggregation.default.ts",
                  "with": "src/environments/ontology-aggregation.faidare.ts"
                },
                {
                  "replace": "src/environments/header.default.ts",
                  "with": "src/environments/header.faidare.ts"
                },
                {
                  "replace": "src/environments/footer.default.ts",
                  "with": "src/environments/footer.faidare.ts"
                },
                {
                  "replace": "src/environments/basket.default.ts",
                  "with": "src/environments/basket.actual.ts"
                },
                {
                  "replace": "src/environments/select-all-results.default.ts",
                  "with": "src/environments/select-all-results.actual.ts"
                },
                {
                  "replace": "src/environments/map-container.default.ts",
                  "with": "src/environments/map-container.faidare.ts"
                }
              ],
              "stylePreprocessorOptions": {
                "includePaths": [
                  "src/assets/faidare/"
                ],
                "sass": {
                  "silenceDeprecations": [
                    "import"
                  ]
                }
              },
              "assets": [
                {
                  "glob": "*",
                  "input": "src/assets/faidare",
                  "followSymlinks": true,
                  "ignore": [
                    "**/*.scss"
                  ],
                  "output": "/assets"
                }
              ]
            }
          },
          "defaultConfiguration": "development"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "options": {
            "proxyConfig": "proxy.conf.mjs"
          },
          "configurations": {
            "production": {
              "buildTarget": "data-discovery-frontend:build:production"
            },
            "development": {
              "buildTarget": "data-discovery-frontend:build:development"
            },
            "rare": {
              "buildTarget": "data-discovery-frontend:build:development,rare"
            },
            "rare-production": {
              "buildTarget": "data-discovery-frontend:build:production,rare-production"
            },
            "brc4env": {
              "buildTarget": "data-discovery-frontend:build:development,brc4env"
            },
            "brc4env-production": {
              "buildTarget": "data-discovery-frontend:build:production,brc4env-production"
            },
            "wheatis": {
              "buildTarget": "data-discovery-frontend:build:development,wheatis"
            },
            "wheatis-production": {
              "buildTarget": "data-discovery-frontend:build:production,wheatis-production"
            },
            "faidare": {
              "buildTarget": "data-discovery-frontend:build:development,faidare"
            },
            "faidare-production": {
              "buildTarget": "data-discovery-frontend:build:production,faidare-production"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "browsers": "ChromeHeadlessNoSandbox",
            "main": "src/test.ts",
            "polyfills": [
              "@angular/localize/init"
            ],
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.cjs",
            "stylePreprocessorOptions": {
              "sass": {
                "silenceDeprecations": [
                  "import"
                ]
              },
              "includePaths": [
                "src/assets/rare/"
              ]
            },
            "styles": [
              "src/styles.scss"
            ],
            "assets": [
              {
                "glob": "*",
                "input": "src/assets/rare",
                "output": "/assets"
              },
              "src/assets"
            ]
          },
          "configurations": {
            "test-multi-browsers": {
              "browsers": "ChromeHeadlessNoSandbox,FirefoxHeadless"
            }
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.html",
              "e2e/**/*.ts"
            ],
            "maxWarnings": 0
          }
        },
        "e2e": {
          "builder": "playwright-ng-schematics:playwright",
          "options": {
            "devServerTarget": "data-discovery-frontend:serve:faidare-production"
          }
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "style": "scss",
      "type": "component"
    },
    "@schematics/angular:directive": {
      "type": "directive"
    },
    "@schematics/angular:service": {
      "type": "service"
    },
    "@schematics/angular:guard": {
      "typeSeparator": "."
    },
    "@schematics/angular:interceptor": {
      "typeSeparator": "."
    },
    "@schematics/angular:module": {
      "typeSeparator": "."
    },
    "@schematics/angular:pipe": {
      "typeSeparator": "."
    },
    "@schematics/angular:resolver": {
      "typeSeparator": "."
    }
  }
}
