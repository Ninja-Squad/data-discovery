{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "data-discovery-frontend": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "dd",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/data-discovery-frontend",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              { "glob": "**/*", "input": "src/assets/rare", "output": "/assets" },
              "src/assets"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/assets/rare/"
              ]
            },
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [],
            "baseHref": "./"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "800kb",
                  "maximumError": "1mb"
                }
              ]
            },
            "rare": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.rare.ts"
                }
              ],
              "baseHref": "/rare-dev/"
            },
            "rare-production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.rare.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "800kb",
                  "maximumError": "1mb"
                }
              ]
            },
            "wheatis": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.wheatis.ts"
                }
              ],
              "baseHref": "/wheatis-dev/",
              "stylePreprocessorOptions": {
                "includePaths": [
                  "src/assets/wheatis/"
                ]
              },
              "assets": [
                { "glob": "*", "input": "src/assets/wheatis", "output": "/assets" },
                "src/assets"
              ]
            },
            "wheatis-production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.wheatis.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "baseHref": "./",
              "stylePreprocessorOptions": {
                "includePaths": [
                  "src/assets/wheatis/"
                ]
              },
              "assets": [
                { "glob": "*", "input": "src/assets/wheatis", "output": "/assets" },
                "src/assets"
              ],
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "800kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "data-discovery-frontend:build:rare",
            "proxyConfig": "proxy.conf.js"
          },
          "configurations": {
            "production": {
              "browserTarget": "data-discovery-frontend:build:production"
            },
            "rare": {
              "browserTarget": "data-discovery-frontend:build:rare"
            },
            "rare-production": {
              "browserTarget": "data-discovery-frontend:build:rare-production"
            },
            "wheatis": {
              "browserTarget": "data-discovery-frontend:build:wheatis"
            },
            "wheatis-production": {
              "browserTarget": "data-discovery-frontend:build:wheatis-production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "data-discovery-frontend:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "browsers": "ChromeHeadlessNoSandbox",
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/assets/rare/"
              ]
            },
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              { "glob": "*", "input": "src/assets/rare", "output": "/assets" },
              "src/assets"
            ]
          },
          "configurations": {
            "allBrowsers": {
              "browsers": "ChromeHeadlessNoSandbox,FirefoxHeadless"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "data-discovery-frontend-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "data-discovery-frontend:serve"
          },
          "configurations": {
            "served": {
              "devServerTarget": ""
            },
            "production": {
              "devServerTarget": "data-discovery-frontend:serve:rare-production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "styleext": "scss"
    }
  },
  "defaultProject": "data-discovery-frontend"
}
