{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "data-discovery-frontend": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "dd",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/data-discovery-frontend",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "src/assets/rare",
                "output": "/assets"
              },
              "src/assets"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/assets/rare/"
              ]
            },
            "styles": [
              "src/styles.scss",
              "node_modules/prismjs/themes/prism-okaidia.css",
              "node_modules/prismjs/plugins/line-numbers/prism-line-numbers.css"
            ],
            "scripts": [
              "node_modules/prismjs/prism.js",
              "node_modules/prismjs/components/prism-json.js",
              "node_modules/prismjs/plugins/line-numbers/prism-line-numbers.js"
            ],
            "baseHref": "./",
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "900kb",
                  "maximumError": "1.2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            },
            "rare": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.rare.ts"
                }
              ],
              "baseHref": "/rare-dev/"
            },
            "rare-production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.rare.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "900kb",
                  "maximumError": "1.2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            },
            "brc4env": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.brc4env.ts"
                },
                {
                  "replace": "src/environments/rare-no-basket.ts",
                  "with": "src/environments/rare-with-basket.ts"
                }
              ],
              "baseHref": "/brc4env-dev/",
              "assets": [
                {
                  "glob": "*",
                  "input": "src/assets/brc4env",
                  "output": "/assets"
                },
                "src/assets"
              ]
            },
            "brc4env-production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.brc4env.prod.ts"
                },
                {
                  "replace": "src/environments/rare-no-basket.ts",
                  "with": "src/environments/rare-with-basket.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "assets": [
                {
                  "glob": "*",
                  "input": "src/assets/brc4env",
                  "output": "/assets"
                },
                "src/assets"
              ],
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "900kb",
                  "maximumError": "1.2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            },
            "wheatis": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.wheatis.ts"
                }
              ],
              "baseHref": "/wheatis-dev/",
              "stylePreprocessorOptions": {
                "includePaths": [
                  "src/assets/wheatis/"
                ]
              },
              "assets": [
                {
                  "glob": "*",
                  "input": "src/assets/wheatis",
                  "output": "/assets"
                },
                "src/assets"
              ]
            },
            "wheatis-production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.wheatis.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "baseHref": "./",
              "stylePreprocessorOptions": {
                "includePaths": [
                  "src/assets/wheatis/"
                ]
              },
              "assets": [
                {
                  "glob": "*",
                  "input": "src/assets/wheatis",
                  "output": "/assets"
                },
                "src/assets"
              ],
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "900kb",
                  "maximumError": "1.2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            },
            "data-discovery": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.data-discovery.ts"
                }
              ],
              "baseHref": "/data-discovery-dev/",
              "stylePreprocessorOptions": {
                "includePaths": [
                  "src/assets/data-discovery/"
                ]
              },
              "assets": [
                {
                  "glob": "*",
                  "input": "src/assets/data-discovery",
                  "output": "/assets"
                },
                "src/assets"
              ],
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "900kb",
                  "maximumError": "1.2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            },
            "data-discovery-production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.data-discovery.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "baseHref": "./",
              "stylePreprocessorOptions": {
                "includePaths": [
                  "src/assets/data-discovery/"
                ]
              },
              "assets": [
                {
                  "glob": "*",
                  "input": "src/assets/data-discovery",
                  "output": "/assets"
                },
                "src/assets"
              ],
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "900kb",
                  "maximumError": "1.2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "data-discovery-frontend:build:rare",
            "proxyConfig": "proxy.conf.js"
          },
          "configurations": {
            "production": {
              "browserTarget": "data-discovery-frontend:build:production"
            },
            "rare": {
              "browserTarget": "data-discovery-frontend:build:rare"
            },
            "rare-production": {
              "browserTarget": "data-discovery-frontend:build:rare-production"
            },
            "brc4env": {
              "browserTarget": "data-discovery-frontend:build:brc4env"
            },
            "brc4env-production": {
              "browserTarget": "data-discovery-frontend:build:brc4env-production"
            },
            "wheatis": {
              "browserTarget": "data-discovery-frontend:build:wheatis"
            },
            "wheatis-production": {
              "browserTarget": "data-discovery-frontend:build:wheatis-production"
            },
            "data-discovery": {
              "browserTarget": "data-discovery-frontend:build:data-discovery"
            },
            "data-discovery-production": {
              "browserTarget": "data-discovery-frontend:build:data-discovery-production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "data-discovery-frontend:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "browsers": "ChromeHeadlessNoSandbox",
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/assets/rare/"
              ]
            },
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              {
                "glob": "*",
                "input": "src/assets/rare",
                "output": "/assets"
              },
              "src/assets"
            ]
          },
          "configurations": {
            "test-multi-browsers": {
              "browsers": "ChromeHeadlessNoSandbox,FirefoxHeadless"
            }
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.html",
              "cypress/**/*.ts"
            ]
          }
        },
        "cypress-run": {
          "builder": "@cypress/schematic:cypress",
          "options": {
            "devServerTarget": "data-discovery-frontend:serve:data-discovery"
          },
          "configurations": {
            "production": {
              "devServerTarget": "data-discovery-frontend:serve:data-discovery-production"
            }
          }
        },
        "cypress-open": {
          "builder": "@cypress/schematic:cypress",
          "options": {
            "watch": true,
            "headless": false
          }
        },
        "e2e": {
          "builder": "@cypress/schematic:cypress",
          "options": {
            "devServerTarget": "data-discovery-frontend:serve:data-discovery",
            "watch": true,
            "headless": false
          },
          "configurations": {
            "production": {
              "devServerTarget": "data-discovery-frontend:serve:data-discovery-production"
            }
          }
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "data-discovery-frontend"
}
