{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "data-discovery-frontend": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "dd",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/data-discovery-frontend",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "src/assets/rare",
                "followSymlinks": true,
                "ignore": ["**/*.scss"],
                "output": "/assets"
              }
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/assets/rare/"
              ]
            },
            "styles": [
              "src/styles.scss",
              "node_modules/prismjs/themes/prism-okaidia.css",
              "node_modules/prismjs/plugins/line-numbers/prism-line-numbers.css"
            ],
            "scripts": [
              "node_modules/prismjs/prism.js",
              "node_modules/prismjs/components/prism-json.js",
              "node_modules/prismjs/plugins/line-numbers/prism-line-numbers.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "900kb",
                  "maximumError": "1.2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "baseHref": "./"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            },
            "rare": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.rare.ts"
                }
              ],
              "baseHref": "/rare-dev/"
            },
            "rare-production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.rare.prod.ts"
                }
              ]
            },
            "brc4env": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.brc4env.ts"
                }
              ],
              "baseHref": "/brc4env-dev/",
              "stylePreprocessorOptions": {
                "includePaths": [
                  "src/assets/brc4env/"
                ]
              },
              "assets": [
                {
                  "glob": "*",
                  "input": "src/assets/brc4env",
                  "followSymlinks": true,
                  "ignore": ["**/*.scss"],
                  "output": "/assets"
                }
              ]
            },
            "brc4env-production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.brc4env.prod.ts"
                }
              ],
              "stylePreprocessorOptions": {
                "includePaths": [
                  "src/assets/brc4env/"
                ]
              },
              "assets": [
                {
                  "glob": "*",
                  "input": "src/assets/brc4env",
                  "followSymlinks": true,
                  "ignore": ["**/*.scss"],
                  "output": "/assets"
                }
              ]
            },
            "wheatis": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.wheatis.ts"
                }
              ],
              "baseHref": "/wheatis-dev/",
              "stylePreprocessorOptions": {
                "includePaths": [
                  "src/assets/wheatis/"
                ]
              },
              "assets": [
                {
                  "glob": "*",
                  "input": "src/assets/wheatis",
                  "followSymlinks": true,
                  "ignore": ["**/*.scss"],
                  "output": "/assets"
                }
              ]
            },
            "wheatis-production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.wheatis.prod.ts"
                }
              ],
              "stylePreprocessorOptions": {
                "includePaths": [
                  "src/assets/wheatis/"
                ]
              },
              "assets": [
                {
                  "glob": "*",
                  "input": "src/assets/wheatis",
                  "followSymlinks": true,
                  "ignore": ["**/*.scss"],
                  "output": "/assets"
                }
              ]
            },
            "faidare": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.faidare.ts"
                }
              ],
              "baseHref": "/faidare-dev/",
              "stylePreprocessorOptions": {
                "includePaths": [
                  "src/assets/faidare/"
                ]
              },
              "assets": [
                {
                  "glob": "*",
                  "input": "src/assets/faidare",
                  "followSymlinks": true,
                  "ignore": ["**/*.scss"],
                  "output": "/assets"
                }
              ]
            },
            "faidare-production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.faidare.prod.ts"
                }
              ],
              "stylePreprocessorOptions": {
                "includePaths": [
                  "src/assets/faidare/"
                ]
              },
              "assets": [
                {
                  "glob": "*",
                  "input": "src/assets/faidare",
                  "followSymlinks": true,
                  "ignore": ["**/*.scss"],
                  "output": "/assets"
                }
              ]
            }
          },
          "defaultConfiguration": "development"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "proxyConfig": "proxy.conf.js"
          },
          "configurations": {
            "production": {
              "buildTarget": "data-discovery-frontend:build:production"
            },
            "development": {
              "buildTarget": "data-discovery-frontend:build:development"
            },
            "rare": {
              "buildTarget": "data-discovery-frontend:build:development,rare"
            },
            "rare-production": {
              "buildTarget": "data-discovery-frontend:build:production,rare-production"
            },
            "brc4env": {
              "buildTarget": "data-discovery-frontend:build:development,brc4env"
            },
            "brc4env-production": {
              "buildTarget": "data-discovery-frontend:build:production,brc4env-production"
            },
            "wheatis": {
              "buildTarget": "data-discovery-frontend:build:development,wheatis"
            },
            "wheatis-production": {
              "buildTarget": "data-discovery-frontend:build:production,wheatis-production"
            },
            "faidare": {
              "buildTarget": "data-discovery-frontend:build:development,faidare"
            },
            "faidare-production": {
              "buildTarget": "data-discovery-frontend:build:production,faidare-production"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "data-discovery-frontend:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "browsers": "ChromeHeadlessNoSandbox",
            "main": "src/test.ts",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/assets/rare/"
              ]
            },
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              {
                "glob": "*",
                "input": "src/assets/rare",
                "output": "/assets"
              },
              "src/assets"
            ]
          },
          "configurations": {
            "test-multi-browsers": {
              "browsers": "ChromeHeadlessNoSandbox,FirefoxHeadless"
            }
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.html",
              "cypress/**/*.ts"
            ],
            "maxWarnings": 0
          }
        },
        "cypress-run": {
          "builder": "@cypress/schematic:cypress",
          "options": {
            "devServerTarget": "data-discovery-frontend:serve:faidare"
          },
          "configurations": {
            "production": {
              "devServerTarget": "data-discovery-frontend:serve:faidare-production"
            }
          }
        },
        "cypress-open": {
          "builder": "@cypress/schematic:cypress",
          "options": {
            "watch": true,
            "headless": false
          }
        },
        "e2e": {
          "builder": "@cypress/schematic:cypress",
          "options": {
            "devServerTarget": "data-discovery-frontend:serve:faidare",
            "watch": true,
            "headless": false
          },
          "configurations": {
            "production": {
              "devServerTarget": "data-discovery-frontend:serve:faidare-production"
            }
          }
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "style": "scss"
    }
  }
}
